{
  "name": "@mahdiyehnorouzi/ds-stencil",
  "private": true,
  "type": "module",
  "main": "./dist/cjs/index.cjs.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/components.d.ts",
  "unpkg": "www/build/stencil-test.esm.js",
  "version": "0.0.8",
  "description": "Stencil Component Starter",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/components.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs.js"
    },
    "./loader": {
      "types": "./loader/index.d.ts",
      "import": "./loader/index.js"
    },
    "./components/stc-button": {
      "types": "./dist/types/components.d.ts",
      "import": "./dist/components/stc-button.js"
    },
    "./components/stc-modal": {
      "types": "./dist/types/components.d.ts",
      "import": "./dist/components/stc-modal.js"
    },
    "./components/stc-input": {
      "types": "./dist/types/components.d.ts",
      "import": "./dist/components/stc-input.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil-community/postcss": "^2.2.0",
    "@stencil/core": "^4.27.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "autoprefixer": "^10.4.23",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.3.0",
    "tailwindcss": "^3.4.17",
    "workbox-build": "^4.3.1"
  },
  "license": "MIT"
}
